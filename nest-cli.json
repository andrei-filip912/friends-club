{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/post/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/post/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/post",
  "projects": {
    "post": {
      "type": "application",
      "root": "apps/post",
      "entryFile": "main",
      "sourceRoot": "apps/post/src",
      "compilerOptions": {
        "tsConfigPath": "apps/post/tsconfig.app.json"
      }
    },
    "upload": {
      "type": "application",
      "root": "apps/upload",
      "entryFile": "main",
      "sourceRoot": "apps/upload/src",
      "compilerOptions": {
        "tsConfigPath": "apps/upload/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}